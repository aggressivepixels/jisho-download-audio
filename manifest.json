{
  "manifest_version": 2,
  "name": "Jisho Download Audio",
  "version": "1.0",
  "description": "Adds a \"Download audio\" button to Jisho's entries",
  "icons": { "48": "icon.png" },
  "content_scripts": [
    {
      "matches": ["*://jisho.org/search/*"],
      "js": ["add-download-button.js"]
    }
  ]
}
